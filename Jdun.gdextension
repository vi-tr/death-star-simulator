[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://rust-components/target/debug/librust_components.so"
linux.release.x86_64 =   "res://rust-components/target/release/librust_components.so"
windows.debug.x86_64 =   "res://rust-components/target/debug/rust_components.dll"
windows.release.x86_64 = "res://rust-components/target/release/rust_components.dll"
macos.debug =            "res://rust-components/target/debug/librust_components.dylib"
macos.release =          "res://rust-components/target/release/librust_components.dylib"
macos.debug.arm64 =      "res://rust-components/target/debug/librust_components.dylib"
macos.release.arm64 =    "res://rust-components/target/release/librust_components.dylib"
